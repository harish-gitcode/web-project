<%- include("header") -%>

<div id="heading" class="box">
    <h1><%= listTitle %> </h1>
</div>
<div class="box">
    <% for(let i=0;i<Items.length;i++){ %>
    <div class="item">
        <input type="checkbox">
        <p><%=Items[i]%></p>
    </div>
    <%}%>
        <form class="item" method="post" action="/">
            <input type="text" name="Data" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%=listTitle%>>+</button>
    </form>
</div>

<%- include("footer") -%>